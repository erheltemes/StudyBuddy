<div class="container signup">
  <div class="row justify-content-center">
    <div class="card col-md-6 m-1" style="width: 18rem;">
      <div class="card-body">
        <h5 class="card-title text-center">sign-up</h5>
        <form>
          <div class="form-group">
            <label for="inputEmail1">email</label>
            <input type="email" class="form-control" id="inputEmail1" aria-describedby="emailHelp">
          </div>
          <div class="form-group">
            <label for="inputPassword1">password</label>
            <input type="password" class="form-control" id="inputPassword1">
          </div>
          <div class="form-group">
            <label for="inputDisplayName1">display name</label>
            <input type="text" class="form-control" id="inputDisplayName1">
          </div>
          <div class="form-group">
            <label for="inputTitle1">title</label>
            <input type="text" placeholder="ex. student, tutor, instructor, etc." class="form-control" id="inputTitle1">
          </div>
          <div class="form-group">
            <label for="inputCity">city</label>
            <input type="text" placeholder="ex. Paris" class="form-control" id="inputCity">
          </div>
          <div class="form-group">
            <label for="inputState">state</label>
            <input type="text" placeholder="ex. TX" class="form-control" id="inputState">
          </div>

          <p>choose a profile picture</p>
         
          <div class="form-check form-check-inline">
            <input class="form-check-input" type="radio" name="profilePicture" id="catProfilePicture" value="./images/cat.jpg">
            <label class="form-check-label" for="catProfilePicture"><img src="./images/cat.jpg" class="img-thumbnail hostimage" alt="cat">  </label>
          </div>
          <div class="form-check form-check-inline">
            <input class="form-check-input" type="radio" name="profilePicture" id="dogProfilePicture" value="./images/dog.jpg">
            <label class="form-check-label" for="dogProfilePicture"><img src="./images/dog.jpg" class="img-thumbnail hostimage" alt="dog">  </label>
          </div>
          <div class="form-check form-check-inline">
            <input class="form-check-input" type="radio" name="profilePicture" id="dragonfruitProfilePicture" value="./images/dragonfruit.jpg">
            <label class="form-check-label" for="dragonfruitProfilePicture"><img src="./images/dragonfruit.jpg" class="img-thumbnail hostimage" alt="dragonfruit">  </label>
          </div>
          <div class="form-check form-check-inline">
            <input class="form-check-input" type="radio" name="profilePicture" id="jellyfishProfilePicture" value="./images/jellyfish.jpg">
            <label class="form-check-label" for="jellyfishProfilePicture"><img src="./images/jellyfish.jpg" class="img-thumbnail hostimage" alt="jellyfish">  </label>
          </div>
          <div class="form-check form-check-inline">
            <input class="form-check-input" type="radio" name="profilePicture" id="leafProfilePicture" value="./images/leaf.jpg">
            <label class="form-check-label" for="leafProfilePicture"><img src="./images/leaf.jpg" class="img-thumbnail hostimage" alt="leaf">  </label>
          </div>
          <div class="form-check form-check-inline">
            <input class="form-check-input" type="radio" name="profilePicture" id="fruitsaladProfilePicture" value="./images/fruitsalad.jpg">
            <label class="form-check-label" for="fruitsaladProfilePicture"><img src="./images/fruitsalad.jpg" class="img-thumbnail hostimage" alt="fruit salad">  </label>
          </div>
          <div class="form-check form-check-inline">
            <input class="form-check-input" type="radio" name="profilePicture" id="pinkflowerProfilePicture" value="./images/pinkflower.jpg">
            <label class="form-check-label" for="pinkflowerProfilePicture"><img src="./images/pinkflower.jpg" class="img-thumbnail hostimage" alt="pinkflower">  </label>
          </div>
          <div class="form-check form-check-inline">
            <input class="form-check-input" type="radio" name="profilePicture" id="garyProfilePicture" value="./images/gary.jpg">
            <label class="form-check-label" for="garyProfilePicture"><img src="./images/gary.jpg" class="img-thumbnail hostimage" alt="cat">  </label>
          </div><br><br>
        
        
          <button class="btn btn-dark" id="newUser">Submit</button>
          <br><br><a class="text-right" href="/login">I have an account</a>
        </form>
      </div>
    </div>
  </div>
</div>


